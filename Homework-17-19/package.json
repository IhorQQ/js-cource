{
  "name": "homework-17",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:run": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/results,overwrite=false,html=false,json=true",
    "test:run:stableEnv": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/results,overwrite=false,html=false,json=true --env ENV=stableEnv",
    "test:run:brokenEnv": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/results,overwrite=false,html=false,json=true --env ENV=brokenEnv",
    "report:merge": "mochawesome-merge 'cypress/reports/results/*.json' > cypress/reports/mochawesome.json",
    "report:generate": "node cypress/reports/generate-report.js",
    "test:report": "npm run test:run && npm run report:merge && npm run report:generate",
    "test:report:stableEnv": "npm run test:run:stableEnv && npm run report:merge && ENV=stableEnv npm run report:generate",
    "test:report:brokenEnv": "npm run test:run:brokenEnv && npm run report:merge && ENV=brokenEnv npm run report:generate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "cypress": "^13.16.0",
    "mocha-foo-reporter": "^0.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.3.3"
  }
}
